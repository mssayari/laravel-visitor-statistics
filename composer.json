{
    "name": "mssayari/laravel-visitor-tracker",
    "description": "Visitor tracker and statistics for Laravel 5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mojtaba Sayari",
            "email": "ms.sayyari@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.* | 5.6.* | 5.7.*",
        "illuminate/http": "5.5.* | 5.6.* | 5.7.*",
        "illuminate/database": "5.5.* | 5.6.* | 5.7.*",
        "illuminate/bus": "5.5.* | 5.6.* | 5.7.*",
        "illuminate/queue": "5.5.* | 5.6.* | 5.7.*",
        "illuminate/contracts": "5.5.* | 5.6.* | 5.7.*",
        "piwik/device-detector": "^3.9",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 | ^7.0",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "mssayari\\Laravel\\VisitorTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mssayari\\Laravel\\VisitorTracker\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mssayari\\Laravel\\VisitorTracker\\VisitorTrackerServiceProvider"
            ],
            "aliases": {
                "VisitStats": "mssayari\\Laravel\\VisitorTracker\\Facades\\VisitStats"
            }
        }
    }
}
